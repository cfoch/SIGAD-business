	<queryString>
		<![CDATA[SELECT P.nombre as proveedor, I.nombre as insumo, SUM(MT.cantidadmovimiento) as total, to_char(MT.fecha, 'MM') as MES
FROM proveedor P, insumo I, tienda T, proveedorinsumo PI, movimientostienda MT, ordencompra OC, detalleordencompra DOC, loteinsumo LI
WHERE MT.tipomovimiento_id = (select id from tipomovimiento where nombre = 'Entrada Fisica') and P.id = OC.proveedor_id and OC.id = DOC.orden_id and DOC.loteinsumo_id = LI.id and I.id = LI.insumo_id and LI.id = MT.loteinsumo_id and case when $P{idTienda} = 0 then true else MT.tienda_id = $P{idTienda} end
GROUP BY proveedor, insumo, MES;]]>
	</queryString>
